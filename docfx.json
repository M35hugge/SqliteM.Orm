{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "SQLiteM.Abstractions/SQLiteM.Abstractions.csproj",
            "SQLiteM.Orm/SQLiteM.Orm.csproj"
          ]
        }
      ],
      "dest": "docs/api"
    }
  ],
  "build": {
    "content": [
      {
        "src": "docs",
        "dest": ".",
        "files": [
          "**/*.md",
          "**/toc.yml"
        ]
      },
      {
        "src": "docs/api",
        "dest": "docs/api",
        "files": [
          "**/*.yml",
          "**/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "src": "docs",
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/logo.png",
      "_enableSearch": "true"
    }
  }
}
